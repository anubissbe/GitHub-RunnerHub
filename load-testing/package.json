{
  "name": "github-runnerhub-load-testing",
  "version": "1.0.0",
  "description": "Comprehensive load testing framework for GitHub-RunnerHub",
  "main": "load-test-framework.js",
  "scripts": {
    "test": "node load-test-framework.js",
    "test:quick": "node load-test-framework.js --duration 60 --concurrency 10",
    "test:stress": "node load-test-framework.js --duration 600 --concurrency 200",
    "test:throughput": "node load-test-framework.js --throughput 2000",
    "start": "node load-test-framework.js",
    "dev": "nodemon load-test-framework.js",
    "lint": "eslint *.js",
    "format": "prettier --write *.js"
  },
  "keywords": [
    "load-testing",
    "performance",
    "github-actions",
    "docker",
    "scaling"
  ],
  "author": "YOUR_GITHUB_ORG",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0",
    "express": "^5.1.0",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "chalk": "^4.1.2",
    "progress": "^2.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_GITHUB_ORG/GitHub-RunnerHub.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_GITHUB_ORG/GitHub-RunnerHub/issues"
  },
  "homepage": "https://github.com/YOUR_GITHUB_ORG/GitHub-RunnerHub#readme"
}