{
  "name": "github-runnerhub",
  "version": "1.0.0",
  "description": "Enterprise-grade GitHub Actions proxy runner system with ephemeral container execution",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:enhanced": "tsx watch src/index-enhanced.ts",
    "start": "node dist/index.js",
    "start:enhanced": "node dist/index-enhanced.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --config jest.config.comprehensive.js",
    "test:unit": "jest --config jest.config.comprehensive.js --selectProjects unit",
    "test:integration": "jest --config jest.config.comprehensive.js --selectProjects integration",
    "test:e2e": "jest --config jest.config.comprehensive.js --selectProjects e2e",
    "test:security": "jest tests/security --verbose --detectOpenHandles --forceExit",
    "test:load": "artillery run tests/load/artillery-config.yml",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "test:watch": "jest --config jest.config.comprehensive.js --watch",
    "test:coverage": "jest --config jest.config.comprehensive.js --coverage",
    "test:ci": "jest --config jest.config.comprehensive.js --ci --coverage --watchAll=false",
    "test:e2e:runner": "./scripts/run-e2e-tests.sh",
    "test:webhooks": "./scripts/test-enhanced-webhooks.sh",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "github-actions",
    "runner",
    "docker",
    "orchestration"
  ],
  "author": "YOUR_GITHUB_ORG",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/redis": "^4.0.10",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.54.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.1",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^5.2.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dockerode": "^3.3.41",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mocha": "^10.0.10",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "artillery": "^2.0.23",
    "autoprefixer": "^10.4.21",
    "chai": "^5.2.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^30.0.2",
    "joi": "^17.13.3",
    "mocha": "^11.7.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
