{
  "name": "github-runnerhub",
  "version": "1.0.0",
  "description": "Enterprise-grade GitHub Actions proxy runner system with ephemeral container execution",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest tests/e2e --verbose --detectOpenHandles --forceExit",
    "test:e2e:runner": "./scripts/run-e2e-tests.sh",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "github-actions",
    "runner",
    "docker",
    "orchestration"
  ],
  "author": "anubissbe",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.54.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.1",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dockerode": "^3.3.41",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^30.0.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
